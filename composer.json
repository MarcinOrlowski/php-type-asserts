{
    "name": "marcin-orlowski/type-asserts",
    "description": "Data type assertions.",
    "homepage": "https://github.com/MarcinOrlowski/php-type-asserts",
    "version": "2.0.0",
    "keywords": [
        "helper",
        "tests",
        "php8",
        "phpunit",
        "unittests",
        "asserts",
        "typehints",
        "validator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcin Orlowski",
            "homepage": "http://MarcinOrlowski.com/"
        }
    ],
    "support": {
        "source": "https://github.com/MarcinOrlowski/php-type-asserts/",
        "issues": "https://github.com/MarcinOrlowski/php-type-asserts/issues"
    },
    "autoload": {
        "psr-4": {
            "MarcinOrlowski\\TypeAsserts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcinOrlowski\\TypeAsserts\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0||^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpunit/php-code-coverage": "^9.0",
        "phpstan/phpstan": "^1.8"
    }
}
